    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginBottom="@dimen/pad_ud"
        android:layout_marginLeft="@dimen/pad_lr"
        android:layout_marginRight="@dimen/pad_lr">
        <com.goldenasia.lottery.pattern.CowLayout
            android:id="@+id/others"
            style="@style/cow_layout"
            android:layout_marginRight="@dimen/pad_ud"
            app:label="杂牌">
            <TextView
                android:layout_width="50dp"

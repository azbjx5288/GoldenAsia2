<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:id="@+id/main_panel"
             android:layout_width="match_parent"
             android:layout_height="match_parent">
    <TableRow>
        <TextView
            android:id="@+id/single_num"
            style="@style/item_lhc_quick_start"
            android:text="单码"/>
        <TextView
            android:id="@+id/small_single"
            style="@style/item_lhc_quick_start"
            android:text="小单"/>
        <TextView
            android:id="@+id/combine_single"
            style="@style/item_lhc_quick_start"
            android:text="合单"/>
        <TextView
            android:id="@+id/poultry"
            style="@style/item_lhc_quick_start"
            android:layout_weight="2"
            android:text="家畜"/>
        <TextView
            android:id="@+id/beast"
            style="@style/item_lhc_quick_start"
            android:text="野兽"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/double_num"
            style="@style/item_lhc_quick_start"
            android:text="双码"/>
        <TextView
            android:id="@+id/small_double"
            style="@style/item_lhc_quick_start"
            android:text="小双"/>
        <TextView
            android:id="@+id/combine_double"
            style="@style/item_lhc_quick_start"
            android:text="合双"/>
        <TextView
            android:id="@+id/mouse"
            style="@style/item_lhc_quick_start"
            android:text="鼠"/>
        <TextView
            android:id="@+id/dragon"
            style="@style/item_lhc_quick_start"
            android:text="龙"/>
        <TextView
            android:id="@+id/monkey"
            style="@style/item_lhc_quick_start"
            android:text="猴"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/big_num"
            style="@style/item_lhc_quick_start"
            android:text="大码"/>
        <TextView
            android:id="@+id/big_single"
            style="@style/item_lhc_quick_start"
            android:text="大单"/>
        <TextView
            android:id="@+id/combine_big"
            style="@style/item_lhc_quick_start"
            android:text="合大"/>
        <TextView
            android:id="@+id/cow"
            style="@style/item_lhc_quick_start"
            android:text="牛"/>
        <TextView
            android:id="@+id/snake"
            style="@style/item_lhc_quick_start"
            android:text="蛇"/>
        <TextView
            android:id="@+id/chicken"
            style="@style/item_lhc_quick_start"
            android:text="鸡"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/small_num"
            style="@style/item_lhc_quick_start"
            android:text="小码"/>
        <TextView
            android:id="@+id/big_double"
            style="@style/item_lhc_quick_start"
            android:text="大双"/>
        <TextView
            android:id="@+id/combine_small"
            style="@style/item_lhc_quick_start"
            android:text="合小"/>
        <TextView
            android:id="@+id/tiger"
            style="@style/item_lhc_quick_start"
            android:text="虎"/>
        <TextView
            android:id="@+id/horse"
            style="@style/item_lhc_quick_start"
            android:text="马"/>
        <TextView
            android:id="@+id/dog"
            style="@style/item_lhc_quick_start"
            android:text="狗"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/head_0"
            style="@style/item_lhc_quick_start"
            android:text="0头"/>
        <TextView
            android:id="@+id/tail_0"
            style="@style/item_lhc_quick_start"
            android:text="0尾"/>
        <TextView
            android:id="@+id/tail_5"
            style="@style/item_lhc_quick_start"
            android:text="5尾"/>
        <TextView
            android:id="@+id/rabbit"
            style="@style/item_lhc_quick_start"
            android:text="兔"/>
        <TextView
            android:id="@+id/sheep"
            style="@style/item_lhc_quick_start"
            android:text="羊"/>
        <TextView
            android:id="@+id/pig"
            style="@style/item_lhc_quick_start"
            android:text="猪"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/head_1"
            style="@style/item_lhc_quick_start"
            android:text="1头"/>
        <TextView
            android:id="@+id/tail_1"
            style="@style/item_lhc_quick_start"
            android:text="1尾"/>
        <TextView
            android:id="@+id/tail_6"
            style="@style/item_lhc_quick_start"
            android:text="6尾"/>
        <TextView
            android:id="@+id/red"
            style="@style/item_lhc_quick_start_red"
            android:text="红"/>
        <TextView
            android:id="@+id/blue"
            style="@style/item_lhc_quick_start_blue"
            android:text="蓝"/>
        <TextView
            android:id="@+id/green"
            style="@style/item_lhc_quick_start_green"
            android:text="绿"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/head_2"
            style="@style/item_lhc_quick_start"
            android:text="2头"/>
        <TextView
            android:id="@+id/tail_2"
            style="@style/item_lhc_quick_start"
            android:text="2尾"/>
        <TextView
            android:id="@+id/tail_7"
            style="@style/item_lhc_quick_start"
            android:text="7尾"/>
        <TextView
            android:id="@+id/red_single"
            style="@style/item_lhc_quick_start_red"
            android:text="红单"/>
        <TextView
            android:id="@+id/blue_single"
            style="@style/item_lhc_quick_start_blue"
            android:text="蓝单"/>
        <TextView
            android:id="@+id/green_single"
            style="@style/item_lhc_quick_start_green"
            android:text="绿单"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/head_3"
            style="@style/item_lhc_quick_start"
            android:text="3头"/>
        <TextView
            android:id="@+id/tail_3"
            style="@style/item_lhc_quick_start"
            android:text="3尾"/>
        <TextView
            android:id="@+id/tail_8"
            style="@style/item_lhc_quick_start"
            android:text="8尾"/>
        <TextView
            android:id="@+id/red_double"
            style="@style/item_lhc_quick_start_red"
            android:text="红双"/>
        <TextView
            android:id="@+id/blue_double"
            style="@style/item_lhc_quick_start_blue"
            android:text="蓝双"/>
        <TextView
            android:id="@+id/green_double"
            style="@style/item_lhc_quick_start_green"
            android:text="绿双"/>
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/head_4"
            style="@style/item_lhc_quick_start"
            android:text="4头"/>
        <TextView
            android:id="@+id/tail_4"
            style="@style/item_lhc_quick_start"
            android:text="4尾"/>
        <TextView
            android:id="@+id/tail_9"
            style="@style/item_lhc_quick_start"
            android:text="9尾"/>
        <TextView
            android:id="@+id/red_big"
            style="@style/item_lhc_quick_start_red"
            android:text="红大"/>
        <TextView
            android:id="@+id/blue_big"
            style="@style/item_lhc_quick_start_blue"
            android:text="蓝大"/>
        <TextView
            android:id="@+id/green_big"
            style="@style/item_lhc_quick_start_green"
            android:text="绿大"/>
    </TableRow>

    <TableRow>
        <com.goldenasia.lottery.component.DrawableLeftRadioButton
            android:id="@+id/clear"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_weight="3"
            android:background="@drawable/border_frame"
            android:button="@null"
            android:drawableLeft="@drawable/delete_icon_button"
            android:drawablePadding="10dp"
            android:gravity="center_vertical"
            android:text="清除"
            android:textColor="@color/app_font_dark_color"
            android:textSize="@dimen/text_size_small"/>
        <TextView
            android:id="@+id/red_small"
            style="@style/item_lhc_quick_start_red"
            android:text="红小"/>
        <TextView
            android:id="@+id/blue_small"
            style="@style/item_lhc_quick_start_blue"
            android:text="蓝小"/>
        <TextView
            android:id="@+id/green_small"
            style="@style/item_lhc_quick_start_green"
            android:text="绿小"/>
    </TableRow>
</TableLayout>
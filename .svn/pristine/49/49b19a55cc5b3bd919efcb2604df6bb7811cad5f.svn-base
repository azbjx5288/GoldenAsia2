<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="35dp"
    android:fitsSystemWindows="true"
    android:gravity="center"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/head_1"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="账号"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />

    <TextView
        android:id="@+id/head_2"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="不活跃天数"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />

    <TextView
        android:id="@+id/head_3"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="余额"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />

    <TextView
        android:id="@+id/head_4"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="充值"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />

    <TextView
        android:id="@+id/head_5"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="提现"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />

    <TextView
        android:id="@+id/head_6"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="投注"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />


    <TextView
        android:id="@+id/head_7"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="中奖"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />

    <TextView
        android:id="@+id/head_8"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="返点"
        android:textColor="@color/text_dark" />

    <View style="@style/vertical_divide_line" />


    <TextView
        android:id="@+id/head_9"
        android:layout_width="72.5dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="盈亏"
        android:textColor="@color/text_dark" />

</LinearLayout>

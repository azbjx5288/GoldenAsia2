    <!-- 沉浸式通知栏 -->
    <style name="StatusBarTheme" parent="android:Theme.Holo.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:fitsSystemWindows">false</item>
        <item name="colorPrimary">@color/app_main</item>
        <item name="colorPrimaryDark">@color/app_main</item>
        <item name="colorAccent">@color/app_main</item>
    </style>

    <style name="StatusBarNaviTheme" parent="Theme.AppCompat.Light.NoActionBar">

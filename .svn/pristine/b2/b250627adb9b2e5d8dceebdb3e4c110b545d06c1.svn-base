<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:padding="20dp"
        android:orientation="vertical">

        <!--特码-->
        <LinearLayout
            android:id="@+id/te_ma"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="定位胆"
                android:textColor="@color/app_font_dark_color"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/gray">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/game_method_info_padding"
                        android:layout_weight="191"
                        android:background="@color/white"
                        android:gravity="center"
                        android:text="前三定位胆"
                        android:textColor="@color/app_font_dark_color"
                        android:textSize="14sp"
                        android:typeface="serif" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/game_method_info_padding"
                        android:layout_weight="470"
                        android:background="@color/white"
                        android:text="从第一，二，三位任意1个位置或多个位置上选择1个号码，所选号码与相同位置上的开奖号码一致。"
                        android:textColor="@color/app_font_dark_color"
                        android:textSize="12sp"
                        android:typeface="serif" />
                </TableRow>

            </TableLayout>
        </LinearLayout>

        <!--正码-->

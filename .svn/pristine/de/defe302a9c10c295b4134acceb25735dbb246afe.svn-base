<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:padding="20dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_taiwan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:gravity="left"
            android:text="“台湾5分彩”以台湾非常流行的官方彩种“台湾宾果”为开奖源，“台湾宾果”开奖号码共20个，按照从小到大的顺序，从第4个号码至第18个号码，每3个号码作为一组统计和值，并以和值的“个位”数字作为“台湾5分彩”的对应开奖结果。"
            android:textColor="@color/app_font_dark_color"
            android:layout_marginBottom="5dp"
            android:visibility="gone"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/tv_taiguo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:gravity="left"
            android:text="泰国30秒彩是泰国春武里府地区的一种独特彩票玩法，历史悠久，开奖公平，从0-9个号码球中每期开出5个号码球（类似于中国重庆时时彩）。"
            android:textColor="@color/app_font_dark_color"
            android:layout_marginBottom="5dp"
            android:visibility="gone"
            android:textSize="13sp" />
        <TextView
            android:id="@+id/tv_wufencai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:gravity="left"
            android:text="北京五分彩是以北京快乐8为开奖奖源，按照从小到大的顺序，从第4个号码至第18个号码，每3个号码作为一组统计和值，并以 和值的“个位”数字作为“北京五分彩” 的对应开奖结果。"
            android:textColor="@color/app_font_dark_color"
            android:layout_marginBottom="5dp"
            android:visibility="gone"
            android:textSize="13sp" />

        <!--特码-->
        <LinearLayout
            android:id="@+id/te_ma"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="定位胆"
                android:textColor="@color/app_font_dark_color"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/gray">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/game_method_info_padding"
                        android:layout_weight="191"
                        android:background="@color/white"
                        android:gravity="center"
                        android:text="定位胆"
                        android:textColor="@color/app_font_dark_color"
                        android:textSize="14sp"
                        android:typeface="serif" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/game_method_info_padding"
                        android:layout_weight="470"
                        android:background="@color/white"
                        android:text="在万位、千位、百位、十位、个位上的任意位置，选择1个号码作为一注，选号与相同位置上的开奖号码一致，即中奖。"
                        android:textColor="@color/app_font_dark_color"
                        android:textSize="12sp"
                        android:typeface="serif" />
                </TableRow>

            </TableLayout>


        </LinearLayout>

        <!--正码-->
